		<!-- 0-0-0-0 -->
		<ResisState id="0" isDef="1">
		</ResisState>
		<!-- 0-0-0-1 -->
		<ResisState id="10">
			<ResisRelation term1Id="fd_1" term2Id="fd_2" value="0" />
		</ResisState>
		<!-- 0-0-1-0 -->
		<ResisState id="20">
			<ResisRelation term1Id="fd_1" term2Id="fd_3" value="0" />
		</ResisState>
		<!-- 0-0-1-1 -->
		<ResisState id="30">
			<ResisRelation term1Id="fd_1" term2Id="fd_2" value="0" />
			<ResisRelation term1Id="fd_1" term2Id="fd_3" value="0" />
		</ResisState>
		<!-- 0-1-0-0 -->
		<ResisState id="40">
			<ResisRelation term1Id="fd_1" term2Id="fd_4" value="0" />
		</ResisState>
		<!-- 0-1-0-1 -->
		<ResisState id="50">
			<ResisRelation term1Id="fd_1" term2Id="fd_2" value="0" />
			<ResisRelation term1Id="fd_1" term2Id="fd_4" value="0" />
		</ResisState>
		<!-- 0-1-1-0 -->
		<ResisState id="60">
			<ResisRelation term1Id="fd_1" term2Id="fd_3" value="0" />
			<ResisRelation term1Id="fd_1" term2Id="fd_4" value="0" />
		</ResisState>
		<!-- 0-1-1-1 -->
		<ResisState id="70">
			<ResisRelation term1Id="fd_1" term2Id="fd_2" value="0" />
			<ResisRelation term1Id="fd_1" term2Id="fd_3" value="0" />
			<ResisRelation term1Id="fd_1" term2Id="fd_4" value="0" />
		</ResisState>
		<!-- 1-0-0-0 -->
		<ResisState id="80">
			<ResisRelation term1Id="fd_1" term2Id="fd_5" value="0" />
		</ResisState>
		<!-- 1-0-0-1 -->
		<ResisState id="90">
			<ResisRelation term1Id="fd_1" term2Id="fd_2" value="0" />
			<ResisRelation term1Id="fd_1" term2Id="fd_5" value="0" />
		</ResisState>
		<!-- 1-0-1-0 -->
		<ResisState id="100">
			<ResisRelation term1Id="fd_1" term2Id="fd_3" value="0" />
			<ResisRelation term1Id="fd_1" term2Id="fd_5" value="0" />
		</ResisState>
		<!-- 1-0-1-1 -->
		<ResisState id="110">
			<ResisRelation term1Id="fd_1" term2Id="fd_2" value="0" />
			<ResisRelation term1Id="fd_1" term2Id="fd_3" value="0" />
			<ResisRelation term1Id="fd_1" term2Id="fd_5" value="0" />
		</ResisState>
		<!-- 1-1-0-0 -->
		<ResisState id="120">
			<ResisRelation term1Id="fd_1" term2Id="fd_4" value="0" />
			<ResisRelation term1Id="fd_1" term2Id="fd_5" value="0" />
		</ResisState>
		<!-- 1-1-0-1 -->
		<ResisState id="130">
			<ResisRelation term1Id="fd_1" term2Id="fd_2" value="0" />
