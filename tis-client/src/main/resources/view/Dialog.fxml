<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import com.cas.sim.tis.svg.SVGGlyph?>

<fx:root styleClass="dialog" type="VBox" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<HBox fx:id="header" styleClass="header">
			<children>
				<Label fx:id="title" styleClass="font14" />
				<HBox alignment="CENTER_RIGHT" HBox.hgrow="ALWAYS">
					<children>
						<Button styleClass="img-btn" mnemonicParsing="false" onAction="#close">
							<graphic>
								<SVGGlyph name="iconfont.svg.close" fill="#747474" size="10" />
							</graphic>
						</Button>
					</children>
				</HBox>
			</children>
		</HBox>
	</children>
</fx:root>
