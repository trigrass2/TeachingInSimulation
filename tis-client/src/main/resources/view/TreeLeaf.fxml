<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<fx:root spacing="5.0" type="VBox" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<HBox fx:id="leaf" alignment="CENTER_LEFT" prefHeight="50.0" VBox.vgrow="ALWAYS">
			<children>
				<HBox alignment="CENTER_LEFT" styleClass="leaf" HBox.hgrow="ALWAYS">
					<children>
						<Label fx:id="leafName" maxWidth="120.0" textFill="WHITE" HBox.hgrow="ALWAYS">
							<tooltip>
								<Tooltip fx:id="tip" />
							</tooltip>
						</Label>
						<HBox alignment="CENTER_RIGHT" HBox.hgrow="ALWAYS">
							<children>
								<Label fx:id="num" textAlignment="RIGHT" textFill="WHITE" />
							</children>
						</HBox>
					</children>
				</HBox>
			</children>
		</HBox>
		<HBox>
			<children>
				<Pane minWidth="10.0" maxWidth="10.0" />
				<VBox fx:id="children" spacing="5.0" HBox.hgrow="ALWAYS" />
			</children>
		</HBox>
	</children>
</fx:root>
